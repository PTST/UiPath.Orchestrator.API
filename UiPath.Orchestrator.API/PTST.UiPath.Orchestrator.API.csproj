<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Uipath Orchestrator API Wrapper</Title>
    <Description>A UiPath Orchestrator API Wrapper</Description>
    <PackageProjectUrl>https://github.com/PTST/UiPath.Orchestrator.API</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PTST/UiPath.Orchestrator.API</RepositoryUrl>
    <PackageTags>UiPath;Orchestrator;API</PackageTags>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <VersionPrefix>0.0.1</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Macross.Json.Extensions" Version="3.0.0" />
    <PackageReference Include="RestSharp" Version="108.0.2" />
  </ItemGroup>

</Project>
